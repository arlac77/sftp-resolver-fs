{
  "name": "sftp-resolver-fs",
  "scripts": {
    "cover": "nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c -- src/sftp-scheme.js",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.1.0",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "semantic-release": "^8.0.0",
    "ssh2": "^0.5.5",
    "stream-equal": "^1.0.1",
    "xo": "^0.19.0"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "version": "0.0.0-semantic-release",
  "description": "resolves sftp urls",
  "main": "dist/sftp-scheme.js",
  "module": "src/sftp-scheme.js",
  "keywords": [
    "url",
    "sftp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/sftp-resolver-fs.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/sftp-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/sftp-resolver-fs#readme",
  "dependencies": {
    "ssh2-sftp-client": "^2.0.0",
    "url-resolver-fs": "^5.2.2"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "presets": [
      "stage-3"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}

{
  "name": "sftp-resolver-fs",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/sftp-scheme.js",
  "module": "src/sftp-scheme.js",
  "description": "resolves sftp urls",
  "keywords": [
    "url",
    "sftp"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/sftp-scheme.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/sftp-scheme.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "ssh2-sftp-client": "^3.1.0",
    "url-resolver-fs": "^7.3.6"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "documentation": "^9.1.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-executable": "^1.3.0",
    "semantic-release": "^15.13.1",
    "ssh2": "^0.6.1",
    "stream-equal": "^1.1.1",
    "travis-deploy-once": "^5.0.11",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/sftp-resolver-fs.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/sftp-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/sftp-resolver-fs#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}

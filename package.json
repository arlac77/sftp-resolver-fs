{
  "name": "sftp-resolver-fs",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/sftp-scheme.js",
  "module": "src/sftp-scheme.js",
  "description": "resolves sftp urls",
  "keywords": [
    "url",
    "sftp"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/sftp-scheme.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/sftp-scheme.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "ssh2-sftp-client": "^3.1.0",
    "url-resolver-fs": "^7.3.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
    "ava": "^1.0.0-rc.1",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.1.0",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "semantic-release": "^15.10.7",
    "ssh2": "^0.6.1",
    "stream-equal": "^1.1.1",
    "travis-deploy-once": "^5.0.9"
  },
  "engines": {
    "node": ">=10.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/sftp-resolver-fs.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/sftp-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/sftp-resolver-fs#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}

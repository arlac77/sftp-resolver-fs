{
  "name": "sftp-resolver-fs",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/sftp-scheme.mjs",
  "exports": {
    ".": "./src/sftp-scheme.mjs"
  },
  "description": "resolves sftp urls",
  "keywords": [
    "sftp",
    "url"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/sftp-scheme.mjs --section=API && documentation readme ./src/sftp-scheme.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/sftp-scheme.mjs && documentation lint ./src/sftp-scheme.mjs"
  },
  "dependencies": {
    "ssh2-sftp-client": "^5.1.0",
    "url-resolver-fs": "^8.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0",
    "stream-equal": "^2.0.0"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/sftp-resolver-fs.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/sftp-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/sftp-resolver-fs#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
